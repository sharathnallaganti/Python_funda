   "source": [
    "def climbStairs(n: int) -> int:\n",
    "    if n <= 2:\n",
    "        return n\n",
    "\n",
    "    prev2, prev1 = 1, 2\n",
    "    for _ in range(3, n + 1):\n",
    "        curr = prev1 + prev2\n",
    "        prev2 = prev1\n",
    "        prev1 = curr\n",
    "\n",
    "    return prev1\n"
   ]
  }

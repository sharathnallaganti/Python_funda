   "source": [
    "class Solution:\n",
    "    def hasCycle(self, head) -> bool:\n",
    "        slow = fast = head\n",
    "\n",
    "        while fast and fast.next:\n",
    "            slow = slow.next\n",
    "            fast = fast.next.next\n",
    "\n",
    "            if slow == fast:\n",
    "                return True\n",
    "\n",
    "        return False\n"
   ]
  }

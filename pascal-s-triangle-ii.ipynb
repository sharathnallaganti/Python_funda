   "source": [
    "class Solution:\n",
    "    def getRow(self, rowIndex: int):\n",
    "        row = [1] * (rowIndex + 1)\n",
    "        \n",
    "        for k in range(1, rowIndex):\n",
    "            row[k] = row[k - 1] * (rowIndex - k + 1) // k\n",
    "        \n",
    "        return row\n"
   ]
